$("#superior-request").html("<%= escape_javascript(render 'superior_request') %>");
$("#superior-request").modal("show");

$('[class^="checkbox-"]').each(function(i){
  $('.checkbox-' + i).change(function(){
    if($('.is-form-' + i).prop('disabled')){
      $('.is-form-' + i).prop('disabled', false)
    }else{
      $('.is-form-' + i).prop('disabled', true)
    }
  })
})

